{
  "name": "Docketeer",
  "version": "10.0.0",
  "license": "MIT",
  "description": "A modern GUI for Docker that provides developers with both management and visualization tools.",
  "author": "Docketeer",
  "main": "dist/DocketeerElectron.js",
  "scripts": {
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack.electron.config.js --mode development && electron .",
    "dev:react": "cross-env NODE_ENV=development webpack-dev-server --config webpack.react.config.js --mode development",
    "dev": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --config webpack.react.config.js --mode development --hot\" \"nodemon server/server.ts\" \"npm run dev:electron\"",
    "buildWeb": "webpack --config=./webpack.react.config.js",
    "test": "jest --verbose",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@testing-library/user-event": "^14.4.3",
    "child_process": "^1.0.2",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "electron-squirrel-startup": "^1.0.0",
    "node": "^19.2.0",
    "os": "^0.1.2",
    "os-utils": "^0.0.14",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-error-overlay": "^6.0.11",
    "react-modal": "^3.15.1",
    "react-redux": "^8.0.4",
    "react-router": "^6.4.1",
    "react-router-dom": "^6.4.1",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "regenerator-runtime": "^0.13.11"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-dmg": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "@jest/globals": "^29.3.1",
    "@redux-devtools/core": "^3.13.1",
    "@reduxjs/toolkit": "^1.9.0",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@types/os-utils": "^0.0.1",
    "@types/pg": "^8.6.5",
    "@types/react": "^18.0.21",
    "@types/react-csv": "^1.1.3",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "autoprefixer": "^10.4.13",
    "axios": "^1.0.0",
    "babel-loader": "^8.2.5",
    "bcryptjs": "^2.4.3",
    "chart.js": "^3.9.1",
