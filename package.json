{
  "name": "docketeer",
  "version": "6.0.0",
  "license": "MIT",
  "description": "A Docker Visualizer",
  "author": "Team Docketeer",
  "proxy": "http://localhost:3000",
  "main": "src/main/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently 'electron-webpack dev' 'nodemon server/server.js'",
    "compile": "electron-webpack && tsc",
    "dist": "npm run compile && electron-builder",
    "dist-win": "npm run compile && electron-builder --win --x64",
    "dist:dir": "yarn dist  --dir -c.compression=store -c.mac.identity=null",
    "temp": "webpack-dev-server --open --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage && open ./coverage/lcov-report/index.html",
    "lint": "eslint --fix",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.2",
    "chart.js": "^2.9.3",
    "clsx": "^1.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "electron-fetch": "^1.7.3",
    "express": "^4.17.1",
    "fix-path": "^3.0.0",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.6.2",
    "parse": "^3.3.0",
    "pg": "^8.7.1",
    "qs": "^6.7.0",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.10.0",
    "react-dom": "^16.13.1",
    "react-modal": "^3.14.2",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.2",
    "source-map-support": "^0.5.19",
    "style-loader": "^1.2.1",
    "twilio": "^3.50.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.10.4",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "babel-loader": "^8.2.1",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^6.2.0",
    "css-loader": "^4.3.0",
    "electron": "^11.0.0",
    "electron-builder": "^22.4.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^8.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.7.1",
    "react-test-renderer": "^16.13.1",
    "sass-loader": "^10.2.0",
    "typescript": "^3.9.10",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "electronWebpack": {
    "whiteListedModules": [
      "react-redux"
    ],
    "main": {
      "sourceDirectory": "src/main"
    },
    "renderer": {
      "sourceDirectory": "src/renderer",
      "template": "src/renderer/index.html"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/docketeer.git"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "build": {
    "extraResources": [
      {
        "from": "./src/database/",
        "to": "database",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
