{
  "name": "docketeer",
  "version": "7.0.0",
  "license": "MIT",
  "description": "A Docker Visualizer",
  "author": "Team Docketeer",
  "main": "dist/DocketeerElectron.js",
  "scripts": {
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack.electron.config.js --mode development && electron .",
    "dev:react": "cross-env NODE_ENV=development webpack-dev-server --config webpack.react.config.js --mode development",
    "dev": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --config webpack.react.config.js --mode development\" \"nodemon server/server.js\" \"npm run dev:electron\"",
    "buildWeb": "webpack --config=./webpack.react.config.js",
    "test": "jest --verbose"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.8",
    "@mui/x-data-grid": "^5.17.7",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.8.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "child_process": "^1.0.2",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "node": "^19.2.0",
    "pg": "^8.8.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-error-overlay": "^6.0.11",
    "react-modal": "^3.15.1",
    "react-redux": "^8.0.4",
    "react-router": "^6.4.1",
    "react-router-dom": "^6.4.1",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "regenerator-runtime": "^0.13.11"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.3.1",
    "@mui/system": "^5.10.10",
    "@redux-devtools/core": "^3.13.1",
    "@reduxjs/toolkit": "^1.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.3",
    "@types/pg": "^8.6.5",
    "@types/react-csv": "^1.1.3",
    "axios": "^1.0.0",
    "babel-loader": "^8.2.5",
    "bcryptjs": "^2.4.3",
    "chart.js": "^3.9.1",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.3",
    "electron": "^21.0.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-reloader": "^1.2.3",
    "express": "^4.18.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.2.1",
    "jest-fetch-mock": "^3.0.3",
    "node-fetch": "^3.2.10",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.20",
    "react-devtools": "^4.26.0",
    "react-password-strength-bar": "^0.4.1",
    "react-test-renderer": "^18.2.0",
    "style-loader": "^3.3.1",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "twilio": "^3.82.1",
    "typescript": "^4.9.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/docketeer.git"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "preset": "ts-jest",
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
