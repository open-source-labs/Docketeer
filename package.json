    "dotenv": "^16.0.3",
    "electron": "^21.0.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-reloader": "^1.2.3",
    "express": "^4.18.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.2.1",
    "jest-fetch-mock": "^3.0.3",
    "node-fetch": "^3.2.10",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "react-devtools": "^4.26.0",
    "react-test-renderer": "^18.2.0",
    "style-loader": "^3.3.1",
    "supertest": "^6.3.0",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
